  <PropertyGroup>
    <PostBuildEvent>REM define msc mods dir if not defined in environment.
IF "%25MSC_DIR%25" == "" (
  SET "MSC_DIR=F:\SteamLibrary\steamapps\common\My Summer Car\Mods"
)
IF NOT EXIST "%25MSC_DIR%25" (
  ECHO Error. mods dir not found. update MSC_DIR env var.
  ECHO dir. %25MSC_DIR%25
)

IF "%25MWC_DIR%25" == "" (
  SET "MWC_DIR=F:\SteamLibrary\steamapps\common\My Winter Car\Mods"
)
IF NOT EXIST "%25MWC_DIR%25" (
  ECHO Error. mods dir not found. update MWC_DIR env var.
  ECHO dir. %25MWC_DIR%25
)

ECHO Config $(ConfigurationName)

IF EXIST "%25MSC_DIR%25" (
 ECHO MSC Mods directory: %25MSC_DIR%25
 ECHO Copying $(TargetPath) -^&gt; %25MSC_DIR%25\$(TargetName).dll
 COPY "$(TargetPath)" "%25MSC_DIR%25" /y &gt;nul

  IF /i "$(ConfigurationName)" == "debug" (
      GOTO msc_dbg
  )
  IF /i "$(ConfigurationName)" == "upd_ver_debug" (
      GOTO msc_dbg
  )
)

GOTO mwc

:msc_dbg
IF EXIST "%25MSC_DIR%25\debug.bat" (
  ECHO Copying $(TargetDir)$(TargetName).pdb -^&gt; %25MSC_DIR%25\$(TargetName).pdb
  COPY "$(TargetDir)$(TargetName).pdb" "%25MSC_DIR%25" &gt;nul
  CD "%25MSC_DIR%25"
  CALL "%25MSC_DIR%25\debug.bat"
  DEL "%25MSC_DIR%25\$(TargetName).pdb" /q &gt;nul
) ELSE (
   ECHO Error. %25MSC_DIR%25\debug.bat not found. Are you in Debug mode?
)

:mwc
IF EXIST "%25MWC_DIR%25" (
 ECHO MWC Mods directory: %25MWC_DIR%25
 ECHO Copying $(TargetPath) -^&gt; %25MWC_DIR%25\$(TargetName).dll
 COPY "$(TargetPath)" "%25MWC_DIR%25" /y &gt;nul

  IF /i "$(ConfigurationName)" == "debug" (
      GOTO mwc_dbg
  )
  IF /i "$(ConfigurationName)" == "upd_ver_debug" (
      GOTO mwc_dbg
  )
)

EXIT /b 0

:mwc_dbg
IF EXIST "%25MWC_DIR%25\debug.bat" (
  ECHO Copying $(TargetDir)$(TargetName).pdb -^&gt; %25MWC_DIR%25\$(TargetName).pdb
  COPY "$(TargetDir)$(TargetName).pdb" "%25MWC_DIR%25" &gt;nul
  CD "%25MWC_DIR%25"
  CALL "%25MWC_DIR%25\debug.bat"
  DEL "%25MWC_DIR%25\$(TargetName).pdb" /q &gt;nul
) ELSE (
   ECHO Error. %25MWC_DIR%25\debug.bat not found. Are you in Debug mode?
)
EXIT /b 0</PostBuildEvent>
